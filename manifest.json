{
    "name": "Quick Lorem",
    "description": "Get 'Lorem Ipsum' single paragraph instantly",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_title": "Click to get text"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": ["scripting", "activeTab"],
    "background": {
      "service_worker": "background.js"
    }
}
